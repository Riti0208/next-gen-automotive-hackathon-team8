# DashScope (Qwen API)
# Get your API key from https://dashscope.console.aliyun.com/
DASHSCOPE_API_KEY=

# PostgreSQL
# Local (Docker Compose)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/car_ai
# Production (Vercel Postgres) - will be auto-configured by Vercel
# DATABASE_URL=postgres://...

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase
# Get your project URL and anon key from https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Google Maps
# Get your API key from https://console.cloud.google.com/google/maps-apis
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=
